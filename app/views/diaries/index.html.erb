<p id="notice"><%= notice %></p>

<h1>Listing Diaries</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Narrative</th>
      <th>Sleep score</th>
      <th>Body weight</th>
      <th>Step count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diaries.each do |diary| %>
      <tr>
        <td><%= diary.date %></td>
        <td><%= diary.narrative %></td>
        <td><%= diary.sleep_score %></td>
        <td><%= diary.body_weight %></td>
        <td><%= diary.step_count %></td>
        <td><%= link_to 'Show', diary %></td>
        <td><%= link_to 'Edit', edit_diary_path(diary) %></td>
        <td><%= link_to 'Destroy', diary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Diary', new_diary_path %>
